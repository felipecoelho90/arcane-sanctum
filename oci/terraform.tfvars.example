# Your OCI Tenancy OCID (found in your OCI Console under Administration > Tenancy Details)
tenancy_ocid        = "ocid1.tenancy.oc1..your_tenancy_ocid"

# Your User OCID (found in your OCI Console under Identity > Users > Your User)
user_ocid           = "ocid1.user.oc1..your_user_ocid"

# Your API Key Fingerprint (found in your OCI Console under Identity > Users > Your User > API Keys)
fingerprint         = "your_api_key_fingerprint"

# Path to your OCI API private key (usually in ~/.oci/oci_api_key.pem)
private_key_path    = "~/.oci/oci_api_key.pem"

# Region (already set to Amsterdam)
region              = "eu-amsterdam-1"

# Path to your SSH public key (usually in ~/.ssh/id_rsa.pub)
ssh_public_key_path = "~/.ssh/id_rsa.pub" 
